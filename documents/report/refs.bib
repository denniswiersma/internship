	issn = {2212-8271},
	journal = {Procedia CIRP},
	keywords = {Reliability Analysis, Conditional Inference Tree, Automobile Engines, Fleet Management, Maintenance},
	note = {51st CIRP Conference on Manufacturing Systems},
	pages = {1392-1397},
