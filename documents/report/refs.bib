	journal = {Nature Communications},
	number = {1},
	pages = {715},
	title = {Transcriptional effects of copy number alterations in a large set of human cancers},
	url = {https://doi.org/10.1038/s41467-020-14605-5},
	volume = {11},
	year = {2020},
