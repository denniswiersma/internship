	doi = {https://doi.org/10.1016/j.jbusres.2019.08.045},
	issn = {0148-2963},
	journal = {Journal of Business Research},
	keywords = {CBBE, COBRAs, Social media, Machine learning, Typology, Consumer brand engagement},
	pages = {594-602},
	title = {Eliciting brand-related social media engagement: A conditional inference tree framework},
	url = {https://www.sciencedirect.com/science/article/pii/S0148296319305156},
